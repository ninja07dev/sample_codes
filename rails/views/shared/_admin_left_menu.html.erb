<%= active_link_to entities_path, wrap_tag: :li do %>
  <% entity_title = t('entity.title') %>
  <i class="menu-icon fa fa-sitemap" title="<%= entity_title %>"></i><%= entity_title %>
<% end %>
<%= active_link_to marketplaces_path, wrap_tag: :li do %>
  <% marketplace_title = t('marketplace.title') %>
