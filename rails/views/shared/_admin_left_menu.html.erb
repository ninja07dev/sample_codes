<% end %>
<%= active_link_to requested_marketplaces_path, wrap_tag: :li do %>
  <% requested_marketplace_title = t('requested_marketplace.title') %>
  <i class="menu-icon fa  fa-plus-square-o" title="<%= requested_marketplace_title %>"></i><%= requested_marketplace_title %>
<% end %>
